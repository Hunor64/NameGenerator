<Window x:Class="Generator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Generator"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="3*"/>
        </Grid.ColumnDefinitions>
        <StackPanel>
            <TextBox x:Name="txtName" Margin="10px"/>
            <RadioButton x:Name="rdbButton" Margin="10px" IsChecked="True">
                <TextBlock Text="Button"/>
            </RadioButton>
            <RadioButton x:Name="rdbTextBlock" Margin="10px">
                <TextBlock Text="TextBlock"/>
            </RadioButton>
            <Button Content="Felvesz" Margin="10px" Click="Button_Click"/>
        </StackPanel>
        <WrapPanel Grid.Column="1" x:Name="panel">
            
        </WrapPanel>
    </Grid>
</Window>
